{
  "name": "nerdbaybackend",
  "version": "1.0.0",
  "description": "api proxy to nerd bay",
  "main": "app.js",
  "scripts": {
    "start": "nodemon ./bin/www",
    "prod": "NODE_ENV=production node ./bin/www",
    "dev": "NODE_ENV=development nodemon ./bin/www",
    "debug": "NODE_ENV=development nodemon --inspect-brk ./bin/www",
    "reinstall": "rm -rf node_modules && npm i",
    "test": "jest --detectOpenHandles",
    "debug-jest": "node --inspect-brk node_modules/.bin/jest --runInBand jest tests/*.js"
  },
  "author": "Drew Palazzari",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "consul": "^0.34.1",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "morgan": "^1.9.1",
    "request-promise": "^4.2.4",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "pryjs": "^1.0.3",
    "superagent": "^5.1.0",
    "supertest": "^4.0.2"
  }
}
